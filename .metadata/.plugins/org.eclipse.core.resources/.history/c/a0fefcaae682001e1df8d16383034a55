package aula06;

import java.util.ArrayList;

public class Lista {


	public static void main(String[] args) {
		
		ArrayList<Double> notas = new ArrayList<Double>();
		
		
		
		notas.add(7.0);
		notas.add(4.0);
		notas.add(6.0);
		notas.add(7.0);
		notas.add(9.0);
		
		for (Double nota: notas)
			System.out.println((nota);
		
		
		
	
	}

}
