package aula06;

import java.util.Stack;

public class Pilha {

	public static void main(String[] args) {
	
		Stack<String> pilha= new Stack<String>();
		
		pilha.add("Aline");
		pilha.add("Pedro");
		pilha.add("Vini");
		pilha.add("Lui");
		pilha.push("Ma");
		
		System.out.println(pilha);
		
		pilha.pop();

		System.out.println(pilha);
		
		
		
		
	}

}
